{
    "tsserver.npm": "/usr/local/bin/npm",
    "tsserver.log": "verbose",
    "coc.preferences.hoverTarget": "preview", 
    "suggest.floatEnable": true,
    "suggest.autoTrigger": "always",
    "coc.preferences.enableFloatHighlight": true,
    "coc.preferences.colorSupport": true,
    "diagnostic.refreshOnInsertMode": true,
    "snippets.ultisnips.directories": ["UltiSnips", "~/.config/coc/ultisnips"],
    "snippets.extends": {
      "cpp": ["c"],
      "htmldjango": ["html"]
    },
    "eslint.options": {
        "configFile": "/Users/yt/.config/configstore/.eslintrc.json"
    },
    "html.enable": true,
    "html.format.enable": true,
    "html.filetypes": [
        "html",
        "htmldjango"
    ],
    "python.autoComplete.extraPaths": [
      "${env:SPARK_HOME}/python",
      "${env:SPARK_HOME}/python/pyspark"
    ],
    "python.formatting.blackPath": "~/.local/bin/black",
    "prettier.printWidth": 60,
    "smartf.timeout": 2000,
    "clangd.path": "/usr/local/opt/llvm/bin/clangd",
    "clangd.enabled": true
}
