{
    "tsserver.npm": "/usr/local/bin/npm",
    "tsserver.log": "verbose",
    "coc.preferences.hoverTarget": "float", 
    "suggest.floatEnable": true,
    "suggest.autoTrigger": "always",
    "coc.preferences.enableFloatHighlight": true,
    "coc.preferences.colorSupport": true,
    "coc.preferences.jumpCommand": "split",
    "diagnostic.refreshOnInsertMode": true,
    "snippets.extends": {
      "javascript.jsx": ["javascript", "html"]
    },
    "eslint.options": {
        "configFile": "/Users/yt/.config/configstore/.eslintrc.json"
    },
    "eslint.filetypes": [
        "javascript", 
        "javascriptreact", 
        "typescript", 
       "typescriptreact"],
    "html.enable": true,
    "html.format.enable": true,
    "html.filetypes": [
        "html",
        "htmldjango"],
    "python.autoComplete.extraPaths": [
      "${env:SPARK_HOME}/python",
      "${env:SPARK_HOME}/python/pyspark" ],
    "python.formatting.blackPath": "~/.local/bin/black",
    "prettier.printWidth": 60,
    "prettier.stylelintIntegration": true,
    "prettier.eslintIntegration": true,
    "smartf.timeout": 1200,
    "languageserver": {
        "haskell": {
          "command": "hie-wrapper",
          "rootPatterns": [".stack.yaml", "cabal.config", "package.yaml"],
          "filetypes": ["hs", "lhs", "haskell"],
          "initializationOptions": {},
          "settings": {
            "languageServerHaskell": {
              "hlintOn": true,
              "maxNumberOfProblems": 10,
              "completionSnippetsOn": true
            }
          }
        }
    }
} 
